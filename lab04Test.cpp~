#include <iostream>
#include <vector>
#include "WordCount.h"
#include "tddFuncs.h"

using namespace std;

int main()
{
  vector<string> vec;
  string one = "hi";
  string two = "gREetings";
  string two = "x-d";
  string three = "-x";
  string four = "as14s";
  string five = "Hi";
  v.push_back(one);
  v.push_back(two);
  v.push_back(three);
  v.push_back(four);
  v.push_back(five);
  v.push_back(five);

  WordCount test;
  for(int i=0;i<vec.size();i++)
    {
      test.incrWordCount(vec[i]);
    }
  cout << "Word count = " << test.getTotalWords() << endl;
  cout << "Unique word count = " << test.getNumUniqueWords() << endl;
  return 0;
}
